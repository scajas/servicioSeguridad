package ec.edu.epn.acreditacion.reportes.dto;

import java.io.Serializable;

public class TotalDTO implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 2628669727322229695L;
	private Long total;
	private String tipo;
	private String descripcion;
	
	
	
	public TotalDTO() {
		super();
		// TODO Auto-generated constructor stub
	}
	public TotalDTO(Long total, String tipo, String descripcion) {
		super();
		this.total = total;
		this.tipo = tipo;
		this.descripcion = descripcion;
	}
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	public String getDescripcion() {
		return descripcion;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	public Long getTotal() {
		return total;
	}
	public void setTotal(Long total) {
		this.total = total;
	}
	

}
