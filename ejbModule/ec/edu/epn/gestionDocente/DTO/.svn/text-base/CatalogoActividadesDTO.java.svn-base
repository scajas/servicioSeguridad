package ec.edu.epn.gestionDocente.DTO;

import java.io.Serializable;

public class CatalogoActividadesDTO implements Serializable{	

	/**
	 * 
	 */
	private static final long serialVersionUID = -2577279464788658089L;
	
	private Integer idReglamentoEvaluacion;
	private String actividad;
	private String asignacionHoras;
	private Integer idTipoActvEval;
	private Double valor;
	private boolean asignar;
	private Double cumplimiento;

	public CatalogoActividadesDTO() {
		super();
	}

	/**
	 * @return the idReglamentoEvaluacion
	 */
	public Integer getIdReglamentoEvaluacion() {
		return idReglamentoEvaluacion;
	}

	/**
	 * @param idReglamentoEvaluacion the idReglamentoEvaluacion to set
	 */
	public void setIdReglamentoEvaluacion(Integer idReglamentoEvaluacion) {
		this.idReglamentoEvaluacion = idReglamentoEvaluacion;
	}

	/**
	 * @return the actividad
	 */
	public String getActividad() {
		return actividad;
	}

	/**
	 * @param actividad the actividad to set
	 */
	public void setActividad(String actividad) {
		this.actividad = actividad;
	}

	/**
	 * @return the asignacionHoras
	 */
	public String getAsignacionHoras() {
		return asignacionHoras;
	}

	/**
	 * @param asignacionHoras the asignacionHoras to set
	 */
	public void setAsignacionHoras(String asignacionHoras) {
		this.asignacionHoras = asignacionHoras;
	}

	/**
	 * @return the idTipoActvEval
	 */
	public Integer getIdTipoActvEval() {
		return idTipoActvEval;
	}

	/**
	 * @param idTipoActvEval the idTipoActvEval to set
	 */
	public void setIdTipoActvEval(Integer idTipoActvEval) {
		this.idTipoActvEval = idTipoActvEval;
	}

	/**
	 * @return the valor
	 */
	public Double getValor() {
		return valor;
	}

	/**
	 * @param valor the valor to set
	 */
	public void setValor(Double valor) {
		this.valor = valor;
	}

	/**
	 * @return the asignar
	 */
	public boolean isAsignar() {
		return asignar;
	}

	/**
	 * @param asignar the asignar to set
	 */
	public void setAsignar(boolean asignar) {
		this.asignar = asignar;
	}

	/**
	 * @return the cumplimiento
	 */
	public Double getCumplimiento() {
		return cumplimiento;
	}

	/**
	 * @param cumplimiento the cumplimiento to set
	 */
	public void setCumplimiento(Double cumplimiento) {
		this.cumplimiento = cumplimiento;
	}

	
	
		
}
