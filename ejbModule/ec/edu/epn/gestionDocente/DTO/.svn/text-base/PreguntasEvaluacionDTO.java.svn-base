package ec.edu.epn.gestionDocente.DTO;

import java.util.ArrayList;
import java.util.List;

public class PreguntasEvaluacionDTO {

	
	private Integer idPreguntasEval;
	private String detalle;	
	private String tipo;
	private String asignacion;
	private List<String> opciones= new ArrayList<String>();
	private String seleccion;
	private String seleccion2;
	private Integer orden;
	private Double totalPonderado;
	
	
	public PreguntasEvaluacionDTO(){
			
		}

	
	public PreguntasEvaluacionDTO(Integer idPreguntasEval, String detalle,  String tipo,
										String asignacion, List<String> opciones, Integer orden, String seleccion,
										String seleccion2, Double totalPonderado) {
		super();
		
		this.idPreguntasEval= idPreguntasEval;
		this.detalle =detalle;
		this.tipo=tipo;
		this.asignacion= asignacion;
		this.opciones= opciones;
		this.orden= orden;
		this.seleccion= seleccion;
		this.seleccion2= seleccion2;
		this.totalPonderado= totalPonderado;
			
	}	
	
	
	/**
	 * @return the idPreguntasEval
	 */
	public Integer getIdPreguntasEval() {
		return idPreguntasEval;
	}
	/**
	 * @param idPreguntasEval the idPreguntasEval to set
	 */
	public void setIdPreguntasEval(Integer idPreguntasEval) {
		this.idPreguntasEval = idPreguntasEval;
	}
	/**
	 * @return the detalle
	 */
	public String getDetalle() {
		return detalle;
	}
	/**
	 * @param detalle the detalle to set
	 */
	public void setDetalle(String detalle) {
		this.detalle = detalle;
	}
	/**
	 * @return the tipo
	 */
	public String getTipo() {
		return tipo;
	}
	/**
	 * @param tipo the tipo to set
	 */
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	/**
	 * @return the asignacion
	 */
	public String getAsignacion() {
		return asignacion;
	}
	/**
	 * @param asignacion the asignacion to set
	 */
	public void setAsignacion(String asignacion) {
		this.asignacion = asignacion;
	}


	/**
	 * @return the orden
	 */
	public Integer getOrden() {
		return orden;
	}


	/**
	 * @param orden the orden to set
	 */
	public void setOrden(Integer orden) {
		this.orden = orden;
	}


	/**
	 * @return the opciones
	 */
	public List<String> getOpciones() {
		return opciones;
	}


	/**
	 * @param opciones the opciones to set
	 */
	public void setOpciones(List<String> opciones) {
		this.opciones = opciones;
	}


	/**
	 * @return the seleccion
	 */
	public String getSeleccion() {
		return seleccion;
	}


	/**
	 * @param seleccion the seleccion to set
	 */
	public void setSeleccion(String seleccion) {
		this.seleccion = seleccion;
	}


	/**
	 * @return the seleccion2
	 */
	public String getSeleccion2() {
		return seleccion2;
	}


	/**
	 * @param seleccion2 the seleccion2 to set
	 */
	public void setSeleccion2(String seleccion2) {
		this.seleccion2 = seleccion2;
	}


	/**
	 * @return the totalPonderado
	 */
	public Double getTotalPonderado() {
		return totalPonderado;
	}


	/**
	 * @param totalPonderado the totalPonderado to set
	 */
	public void setTotalPonderado(Double totalPonderado) {
		this.totalPonderado = totalPonderado;
	}



	
	
}

